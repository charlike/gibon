{
  "name": "koa-better-body",
  "version": "1.0.5",
  "description": "A koa body parser middleware with support for `multipart`, `json`, `csp-report`or `urlencoded` request bodies. Via formidable and co-body.",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test mocha --harmony-generators --require should",
    "coveralls": "NODE_ENV=test node --harmony-generators node_modules/.bin/istanbul cover _mocha --report lcovonly -- --require should && cat ./coverage/lcov.info | coveralls || true",
    "examples-multer": "NODE_ENV=test node --harmony examples/multer.js",
    "examples-koa-router": "NODE_ENV=test node --harmony examples/koa-router.js"
  },
  "repository": "tunnckoCore/koa-better-body",
  "keywords": [
    "koa",
    "request",
    "body",
    "middleware",
    "bodyParser",
    "form",
    "data",
    "formdata",
    "urlencoded",
    "multipart",
    "multiple",
    "upload",
    "json",
    "formidable"
  ],
  "dependencies": {
    "co-body": "*",
    "extend": "2.0.0",
    "formidable": "1.0.15"
  },
  "devDependencies": {
    "koa": "*",
    "koa-router": "*",
    "koa-resource-router": "*",
    "mocha-lcov-reporter": "*",
    "mocha": "*",
    "multiline": "*",
    "should": "*",
    "supertest": "*",
    "coveralls": "*",
    "istanbul-harmony": "~0.3.0",
    "jshint": "*"
  },
  "licenses": [{
    "type": "MIT",
    "url": "https://github.com/tunnckoCore/koa-better-body/blob/master/license.md"
  }],
  "author": {
    "name": "Charlike Mike Reagent",
    "email": "mameto_100@mail.bg"
  }
}
