{
  "name": "koa-better-body",
  "version": "1.0.5",
  "description": "A koa body parser middleware with support for `multipart`, `json`, `csp-report`or `urlencoded` request bodies. Via formidable and co-body.",
  "main": "index.js",
  "scripts": {
    "lint": "npm install && ./node_modules/.bin/jshint **/*.js",
    "test": "npm run lint && NODE_ENV=test ./node_modules/.bin/mocha --require should --harmony-generators",
    "test-cov": "npm run lint && NODE_ENV=test node --harmony-generators node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- -u exports --require should",
    "test-travis": "npm run lint && NODE_ENV=test node --harmony-generators node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha --report lcovonly -- -u exports --require should",
    "examples-multer": "npm run lint && node --harmony examples/multer.js",
    "examples-koa-router": "npm run lint && node --harmony examples/koa-router.js"
  },
  "repository": "tunnckoCore/koa-better-body",
  "keywords": [
    "koa",
    "request",
    "body",
    "middleware",
    "bodyParser",
    "form",
    "data",
    "formdata",
    "urlencoded",
    "multipart",
    "multiple",
    "upload",
    "json",
    "formidable"
  ],
  "dependencies": {
    "co-body": "*",
    "extend": "2.0.0",
    "formidable": "1.0.15"
  },
  "devDependencies": {
    "koa": "*",
    "koa-router": "*",
    "koa-resource-router": "*",
    "mocha-lcov-reporter": "*",
    "mocha": "*",
    "multiline": "*",
    "should": "*",
    "supertest": "*",
    "coveralls": "*",
    "istanbul-harmony": "~0.3.0",
    "jshint": "*"
  },
  "licenses": [{
    "type": "MIT",
    "url": "https://github.com/tunnckoCore/koa-better-body/blob/master/license.md"
  }],
  "author": {
    "name": "Charlike Mike Reagent",
    "email": "mameto_100@mail.bg"
  }
}
